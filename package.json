  "dependencies": {
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.8",
    "pinia": "3.0.3",
    "sass": "1.92.1",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.12.0",
    "@tsconfig/node18": "18.2.4",
    "@types/node": "24.3.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.35.0",
    "eslint-plugin-vue": "10.4.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.2",
    "vite": "7.1.5",
    "vue-tsc": "3.0.6"
